{
  "name": "proctor-app-releases",
  "version": "1.0.0",
  "description": "Proctor App Releases Server",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">= 20.0.0"
  },
  "scripts": {
    "build:tsc": "tsc -p tsconfig.json",
    "build:tscpaths": "tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build": "rimraf dist && yarn run build:tsc && yarn run build:tscpaths",
    "dev": "NODE_ENV=development nodemon -r dotenv/config src/index.ts",
    "format": "prettier src --check --experimental-cli",
    "format:f": "prettier src --write --experimental-cli",
    "lint": "eslint . --ext js,mjs,ts",
    "lint:f": "eslint . --ext js,mjs,ts --fix",
    "postinstall": "husky",
    "start": "NODE_ENV=production node -r source-map-support/register -r dd-trace/init dist/index.js",
    "typecheck": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "hazel-server": "5.1.1",
    "pino": "10.0.0",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@types/node": "~22.10.7",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "nodemon": "3.0.1",
    "pino-pretty": "13.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^3.0.3",
    "rimraf": "3.0.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "tscpaths": "0.0.9",
    "typescript": "5.9.2",
    "typescript-eslint": "8.45.0"
  }
}
